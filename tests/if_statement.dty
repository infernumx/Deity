string a = "abc";

if a == "abc" {
    put("a is abc");
}

a = "ghi";

if a == "abc" {
    put("a is abc");
} else if a == "def" {
    put("a is def");
} else if a == "ghi" {
    put("a is ghi");
}

fn even_check(obj n) : boolean {
    return n == 0;
}

if (6 % 2) | (even_check) {
    put("even");
}